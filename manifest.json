{
  "manifest_version": 2,
  "name": "ndate_gourment",
  "description": "ndate_gourment",
  "version": "1.0",

  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["*://*/*"],
    "js": ["jquery-3.3.1.min.js","main.js"]
  }],
  "permissions": [
    "alarms",
    "tabs", "*://*/*",
    "background",
    "webNavigation"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
